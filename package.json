{
  "name": "lisc",
  "description": "Spellcrafting Calculator for the Phoenix Freeshard Server",
  "version": "0.0.0",
  "author": "Mathias Punkehofer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mpunkenhofer/lisc.git"
  },
  "bugs": {
    "url": "https://github.com/mpunkenhofer/lisc/issues"
  },
  "homepage": "https://mpunkenhofer.github.io/lisc/",
  "scripts": {
    "clean": "gts clean",
    "build": "npm run clean && webpack --config webpack.production.js",
    "start": "npm run clean && webpack serve --config=webpack.development.js",
    "gen-db": "ts-node db/ingest.ts",
    "gen-icons": "py ./bin/svg_to_png.py ./assets/images/lisc.svg -d ./public/ -p lisc -r 16 32 48 180 192 512 && magick public/lisc16.png public/lisc32.png public/lisc48.png public/favicon.ico && rimraf public/lisc16.png public/lisc32.png public/lisc48.png && copyfiles -f assets/images/lisc_mask.svg assets/images/lisc.svg public",
    "predeploy": "npm run build",
    "crawl": "npx ts-node scripts/crawl_items.ts",
    "deploy": "gh-pages -d build",
    "lint": "gts lint",
    "fix": "gts fix"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^5.4.1",
    "@types/glob": "^7.1.3",
    "@types/node": "^15.0.3",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux-logger": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "copyfiles": "^2.4.1",
    "create-file-webpack": "^1.0.2",
    "css-loader": "^5.2.4",
    "eslint": "^7.26.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fast-css-loader": "^1.0.2",
    "fast-sass-loader": "^2.0.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.7",
    "gh-pages": "^3.1.0",
    "glob": "^7.1.7",
    "gts": "^3.1.0",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.6.0",
    "prettier": "2.3.0",
    "puppeteer": "^19.6.3",
    "react-hot-loader": "^4.13.0",
    "redux-logger": "^3.0.6",
    "resolve-url-loader": "^4.0.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^11.1.0",
    "sqlite3": "^5.0.2",
    "ts-loader": "^9.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.2.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-middleware": "^3.7.3",
    "webpack-dev-server": "^4.11.1",
    "webpack-font-preload-plugin": "^1.3.0",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.1",
    "better-sqlite3": "^7.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux-thunk": "^2.3.0"
  }
}
